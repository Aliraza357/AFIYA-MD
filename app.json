{
  "name": "AFIYA-MD",
  "description": "Afiya Multi Device whatsappbot",
  "logo": "https://i.imgur.com/lfcMtYg.jpeg",
  "keywords": ["whatsapp"],
  "repository": "https://github.com/SUHAID-BRO/AFIYA-MD",
  "website": "https://github.com/SUHAID-BRO/AFIYA-MD",
  "success_url": "https://github.com/SUHAID-BRO/AFIYA-MD",
  "stack": "container",
  "env": {
    "ANTI_LINK": {
      "description": "true/false",
      "required": false
    },
    "ANTILINK_ACTION": {
      "description": "kick",
      "required": false
    },
    "RMBG_KEY": {
      "description": "Remove bg api key ",
      "required": false
    },
    "SESSION_ID": {
      "description": "Your Session ID",
      "required": true
    },
    "HANDLERS": {
      "description": "prefix for commands, enter false for no prefix",
      "required": true,
      "value": "."
    },
    "LOGS": {
      "description": "need to show message logs ?",
      "required": false
    },
    "PACKNAME": {
      "description": "Sticker Packname",
      "required": false
    },

    "GOODBYE_MSG": {
      "description": "goodbye Message",
      "required": false
    },
    "AUTHOR": {
      "description": "Sticker Pack AUTHOR",
      "required": false
    },
    "OWNER_NAME": {
      "description": "Owner Name",
      "required": "false",
      "value": "SUHAID"
    },
    "BOT_NAME": {
      "description": "Bot name",
      "required": "false",
      "value": "AFIYA-MD"
     },
     "ALIVE": {
      "description": "Alive Message",
      "required": "false",
      "value": "```i'm active```"
    },
    "SUDO": {
      "description": "sudo numbers with country code seperate with , for multiple numbers",
      "required": false
    },
    "WORK_TYPE": {
      "description": "work type",
      "required": "true",
      "value": "private"
     },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": true
    }
  }
}
